@users.map do |user|
  {
    id: user.id,
    name: user.first_name,
    email: user.email,
    username: user.username,
    userTypeId: user.user_type_id,
    firstName: user.first_name,
    lastName: user.last_name, 
    phone: user.phone,
    birthday: user.birthday,
    employeePositionOutlets: user.employee_position_outlets.map do |epo|
      {
        id: epo.id,
        userId: epo.user_id,
        outletId: epo.outlet_id,
        positionId: epo.position_id,
      }
    end
  }
end